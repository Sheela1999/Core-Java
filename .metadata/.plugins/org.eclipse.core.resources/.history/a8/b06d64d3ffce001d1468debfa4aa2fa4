package com.xworkz.oopsprogramming;

public class Laptop {
	
	String laptopName;
	int price;
	int storage;
	String type;  
	String ram;
	
	public Laptop() {
		System.out.println("Default Constructor");
	}
	
	public void photos(){
		System.out.println("To make a memories");
		
	}
	
	public void files(){
		System.out.println("To Store Data");
	}
	
	public void program(){
		System.out.println("To run java");
	}

}
