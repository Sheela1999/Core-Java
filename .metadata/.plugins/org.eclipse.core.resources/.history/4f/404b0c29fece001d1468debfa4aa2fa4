package com.xworkz.oopsprogramming;

public class Hp extends Laptop{
	
	String genProcessor;
	double size;
	String inputs;
	
	public Hp(String genProcessor, double size, String inputs) {
		this.genProcessor= genProcessor;
		this.size = size;
		this.inputs = inputs;
		
		System.out.println(this.genProcessor);
		System.out.println(this.size);
		System.out.println(this.inputs);
	}
	
	public void speed(){
		System.out.println("good Speed");
	}
	
	public void speakers(){
		System.out.println("It is having two speakers");
	}
	

}
